msgstr "Synkroniserade !name (!uid), senast synkroniserad !interval sen"

#: tzintellitime_sync.module:165
msgid "Expired session for user !name (!uid), last sync !interval ago"
msgstr "Gammal session för !name (!uid), senast synkroniserad !interval sen"

#: tzintellitime_sync.module:173
msgid "Failed to synchronize user !name (!uid), last sync !interval ago"
msgstr "Synkronisering för !name (!uid) misslyckades, senast synkroniserad !interval sen"

#: tzintellitime_sync.module:368
msgid "Removed !rows reports for user !user (!uid) after sync"
msgstr "Tog bort !rows rapporter för användare !user (!uid) efter synkronisering"

#: tzintellitime_sync.module:384
msgid "Sending report with nid !nid and itid !itid to week !week"
msgstr "Skickar rapport med node-id !nid och intellitime-id !itid till vecka !week"

#: tzintellitime_sync.module:413
