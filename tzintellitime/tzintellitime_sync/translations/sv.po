#: tzintellitime_sync.admin.inc:6
msgid "Minutes between synchronizations"
msgstr "Antal minuter mellan synkroniseringar"

#: tzintellitime_sync.admin.inc:7
msgid "Minimum interval in minutes between two synchronizations of the same user. The actual synchronization times are also affected by the current cron-interval."
msgstr "Minsta intervall i minuter mellan två synkroniseringar. Det faktiska intervallet beror även på de nuvarande cron-inställningarna."

#: tzintellitime_sync.admin.inc:12
msgid "Users per synchronization"
msgstr "Antal användare per synkronisering"

#: tzintellitime_sync.admin.inc:13
msgid "Maximum number of users to synchronize per session"
msgstr "Max antal användare att synkronisera i varje omgång."

#: tzintellitime_sync.admin.inc:18
msgid "Number of weeks forward in time to synchronize"
msgstr "Antal veckor framåt att synkronisera"

#: tzintellitime_sync.admin.inc:19
msgid "Enter 0 for only the current week, 1 for the current week and next week, and so on."
msgstr "Skriv in 0 för endast nuvarande vecka, 1 för nuvarande och nästa vecka, och så vidare."

#: tzintellitime_sync.admin.inc:24
msgid "Initial synchronization"
msgstr "Inledande synkronisering"

#: tzintellitime_sync.admin.inc:26
msgid "Do a full synchronization for both new users and users with a expired session."
msgstr "Synkronisera direkt för både nya användare och användare med en ogiltig sessionsnyckel."

#: tzintellitime_sync.admin.inc:27
msgid "Only for new users. Users with expired session are synchronized at the next scheduled synchronization."
msgstr "Endast nya användare. Användare med ogiltig sessionsnyckel synkroniseras vid nästa schemalagda tillfälle."

#: tzintellitime_sync.admin.inc:28
msgid "Never do initial synchronization, always defer to the next scheduled synchronization."
msgstr "Synkronisera aldrig vid inloggning, vänta alltid till nästa schemalagda tillfälle."

#: tzintellitime_sync.admin.inc:30
msgid "Configure how to handle synchronization on login"
msgstr "Konfigurera synkronisering vid inloggning"

#: tzintellitime_sync.admin.inc:45
msgid "Invalid synchronization interval, enter number of minutes."
msgstr "Ogiltigt synkroniseringsintervall, skriv in antal minuter."

#: tzintellitime_sync.admin.inc:47
msgid "Synchronization interval must be greater than 1"
msgstr "Synkroniseringsintervallet måste vara längre än 1 minut"

#: tzintellitime_sync.admin.inc:51
msgid "Enter number of weeks as a positive integer."
msgstr "Skriv in antalet veckor att synkronisera som ett positivt heltal."

#: tzintellitime_sync.admin.inc:53
msgid "Must synchronize at least the current week"
msgstr "Måste synkronizera åtminstone nuvarande vecka"

#: tzintellitime_sync.module:341
msgid "Absence"
msgstr "Frånvaro"

#: tzintellitime_sync.module:133;138;158;168;176;396;412;442
msgid "tzintellitime"
msgstr ""

#: tzintellitime_sync.module:133
msgid "Tried to start synchronization for illegal user id \"@uid\""
msgstr "Försökte synkronisera ogilitigt användar-id \"@uid\""

#: tzintellitime_sync.module:138
msgid "No session available for user !name (!uid), last sync !interval ago"
msgstr "Ogiltig session för !name (!uid), senast synkroniserad !interval sen"

#: tzintellitime_sync.module:158
msgid "Synchronized user !name (!uid), last sync !interval ago"
msgstr "Synkroniserade !name (!uid), senast synkroniserad !interval sen"

#: tzintellitime_sync.module:168
msgid "Expired session for user !name (!uid), last sync !interval ago"
msgstr "Gammal session för !name (!uid), senast synkroniserad !interval sen"

#: tzintellitime_sync.module:176
msgid "Failed to synchronize user !name (!uid), last sync !interval ago"
msgstr "Synkronisering för !name (!uid) misslyckades, senast synkroniserad !interval sen"

#: tzintellitime_sync.module:396
msgid "Removed !rows reports for user !user (!uid) after sync"
msgstr "Tog bort !rows rapporter för användare !user (!uid) efter synkronisering"

#: tzintellitime_sync.module:412
msgid "Sending report with nid !nid and itid !itid to week !week"
msgstr "Skickar rapport med node-id !nid och intellitime-id !itid till vecka !week"

#: tzintellitime_sync.module:442
msgid "Tried to create tzreport without valid jobid"
msgstr "Försökte skapa tzreport utan giltigt jobid"

